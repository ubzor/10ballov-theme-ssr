{
    "name": "10ballov-theme",
    "version": "0.0.1",
    "main": "server/index.ts",
    "license": "MIT",
    "engines": {
        "node": ">=14.17.0"
    },
    "scripts": {
        "start": "yarn run dev",
        "dev": "ts-node ./server",
        "prod": "yarn run build && yarn run prod:serve",
        "build": "yarn run build:clean && yarn run build:vite && yarn run build:prerender",
        "build:clean": "rimraf dist/",
        "build:vite": "vite build && vite build --ssr",
        "build:prerender": "vite-plugin-ssr prerender",
        "prod:serve": "serve dist/client/ --listen 3005",
        "lint": "yarn run lint:scripts && yarn run lint:styles && yarn run lint:markup",
        "lint:scripts": "eslint --ext .ts,vue --ignore-path .eslintignore .",
        "lint:styles": "stylelint ./**/*.{css,scss,vue}",
        "lint:markup": "vue-tsc --noEmit"
    },
    "dependencies": {
        "axios": "^0.24.0",
        "mitt": "^3.0.0",
        "query-string": "^7.0.1",
        "type-fest": "^2.8.0",
        "vue": "^3.2.26",
        "vue-class-component": "^8.0.0-rc.1",
        "vue-i18n": "^9.2.0-beta.23",
        "vue-property-decorator": "^10.0.0-rc.3",
        "vuex": "^4.0.2",
        "vuex-class": "^0.3.2"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/jest": "^27.0.3",
        "@types/node": "^16.11.12",
        "@types/node-fetch": "^3.0.3",
        "@typescript-eslint/eslint-plugin": "^5.7.0",
        "@typescript-eslint/parser": "^5.7.0",
        "@vitejs/plugin-vue": "^2.0.0",
        "@vue/compiler-sfc": "^3.2.26",
        "@vue/eslint-config-prettier": "^6.0.0",
        "@vue/eslint-config-typescript": "^9.1.0",
        "@vue/server-renderer": "^3.2.26",
        "cross-fetch": "^3.1.4",
        "dotenv": "^10.0.0",
        "eslint": "^8.4.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-vue": "^8.2.0",
        "express": "^4.17.1",
        "husky": "^7.0.4",
        "lint-staged": "^12.1.2",
        "node-fetch": "^3.1.0",
        "path": "^0.12.7",
        "postcss-html": "^1.3.0",
        "postcss-scss": "^4.0.2",
        "prettier": "^2.5.1",
        "pug": "^3.0.2",
        "rimraf": "^3.0.2",
        "sass": "^1.45.0",
        "serve": "^13.0.2",
        "stylelint": "^14.1.0",
        "stylelint-config-html": "^1.0.0",
        "stylelint-config-recommended": "^6.0.0",
        "stylelint-config-recommended-vue": "^1.1.0",
        "stylelint-config-standard": "^24.0.0",
        "stylelint-config-standard-scss": "^3.0.0",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.4",
        "vite": "^2.7.2",
        "vite-plugin-ssr": "^0.3.36",
        "vue-tsc": "^0.29.8"
    },
    "imports": {
        "@/*": "./*.ts"
    }
}
